{
    "name": "primalmaxor/laravel-magicpass",
    "description": "Passwordless authentication for Laravel via 4-digit codes.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "primalmaxor",
            "email": "you@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Primalmaxor\\MagicPass\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Primalmaxor\\MagicPass\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Primalmaxor\\MagicPass\\MagicPassServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true
}
